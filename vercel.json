{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "env": {
        "MONGO_URI": "production_mongo_uri_placeholder",
        "PYTHON_API_URL": "production_python_api_url_placeholder"
    }
}